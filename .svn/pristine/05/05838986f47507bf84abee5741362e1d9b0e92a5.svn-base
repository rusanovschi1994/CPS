package copypasteproject;

import javafx.beans.property.SimpleStringProperty;
import javafx.beans.property.StringProperty;

/**
 *
 * @author IT-0
 */
public class FiltredClass {

    private final StringProperty filtredField;

    public FiltredClass(String fField) {
        this.filtredField = new SimpleStringProperty(fField);
    }

    public final StringProperty filtredFieldProperty() {
        return this.filtredField;
    }

    public final java.lang.String getFiltredField() {
        return this.filtredFieldProperty().get();
    }

    public final void setFiltredField(final java.lang.String filtredField) {
        this.filtredFieldProperty().set(filtredField);
    }
}
